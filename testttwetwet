Move.Size = UDim2.new(0, 0, 0, 2)
local function BASCUYN_fake_script() -- Move.Move 
	local script = Instance.new('Script', Move)

	local prog = 0
	
	while wait() do
		prog = prog + 1
		script.Parent.Size = UDim2.new(prog/100, 0, 0.02, 0)
		if prog == 100 then
			wait(0.5)
			script.Parent.Parent.Parent.Parent.Notify:Destroy()
		end
	end
end
coroutine.wrap(BASCUYN_fake_script)()

Ttl = NotifyTitle
Txt = NotifyText
